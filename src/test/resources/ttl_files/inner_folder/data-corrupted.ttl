  rdf:type owl:Ontology ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/s-pipes-lib> ;
.

:bind-firstname
  rdf:type sml:BindWithConstant ;
  sm:next :construct-greeding ;dz
  sm:outputVariable "personName" ;
  sml:value [
        a sp:Expression ;
        sp:text "?firstName" ;
  ] ;
  rdfs:label "Bind person name" ;
.
